{
  "manifest_version": 2,

  "name": "GimmeLink",
  "description":
    "This extension allows a user to share their current page url with a fast disposal shortened url",
  "version": "0.1.1",
  "content_security_policy":
    "script-src 'self' https://www.gstatic.com/; object-src 'self'",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "GimmeLink"
  },
  "permissions": ["activeTab", "storage"]
}
